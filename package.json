{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.5",
    "nodemon": "^2.0.20"
  },
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "install-client": "cd client && yarn add",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn run install-client && yarn run build",
    "start": "node server.js",
    "start-watch": "nodemon server.js",
    "dev": "concurrently \"yarn run start-watch\" \"cd client && yarn start\""
  }
}
